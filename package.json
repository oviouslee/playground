{
  "name": "playground",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "build": "ng build core --configuration development",
    "restore": "dotnet restore ./server -s ./server/nuget-packages",
    "seed": "dotnet run --project ./server/dbseeder -- \"Server=S2P_NAS_2;Database=playground-dev;User Id=sa;Password=$$!S2P.sql!$$;",
